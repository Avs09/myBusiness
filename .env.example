# .env.example
POSTGRES_DB=myBusiness #tu nombre DB en postgres
POSTGRES_USER=postgres #tu usuario DB en postgres 
POSTGRES_PASSWORD=secret #tu contrase√±a DB en postgres

DB_PORT=5433 #tu puerto DB en postgres

SPRING_PROFILES_ACTIVE=dev
SPRING_DATASOURCE_URL=jdbc:postgresql://db:5432/${POSTGRES_DB}
